x=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row justify-content-center\">\n            <div class=\"content-wrap\">\n                <div class=\"col-12 col-lg-8 col-text\">\n                    <div class=\"text-wrapper\">\n                        <p class=\"mbr-text mbr-fonts-style mb-0\" mbr-theme-style=\"display-2\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\">\n                            Turnkey solutions for car, home &amp; office decor.</p>\n                    </div>\n                </div>\n                <div class=\"col-12 col-lg-4 image-wrapper\">\n                    <div class=\"btn-container\">\n                        <div mbr-buttons mbr-theme-style=\"display-7\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a class=\"btn btn-success\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">Enquire today</a></div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
